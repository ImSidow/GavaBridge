{
    "name": "gavabridge/kra",
    "description": "Simple Laravel SDK for KRA (GavaConnect) APIs",
    "type": "library",
    "license": "MIT",
    "version": "1.0.8",
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.6",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "GavaBridge\\Kra\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GavaBridge\\Kra\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GavaBridge\\Kra\\Providers\\KraServiceProvider"
            ],
            "aliases": {
                "GavaBridge": "GavaBridge\\Kra\\Facades\\GavaBridge"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/pest || vendor/bin/phpunit",
        "lint": "vendor/bin/pint"
    },
    "authors": [
        {
            "name": "ImSidow"
        }
    ],
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
